    "build": {
        "target": "pardus-dev",
        "dockerfile": "./Dockerfile"
    } ,
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.defaultProfile.linux": "zsh"
            },
            "extensions": [
                "ms-azuretools.vscode-docker",
                "github.vscode-pull-request-github",
                "github.copilot",
                "yzhang.markdown-all-in-one",
                "redhat.vscode-yaml",
                "esbenp.prettier-vscode"
            ]
        }
    }
}